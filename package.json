{
  "name": "temp-private-2110-new-be-for-fe",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "service2110",
    "taskViewerBe",
    "taskViewerFe"
  ],
  "scripts": {
    "dev": "npm run start:dev -w service2110",
    "build": "npm run build -w service2110",
    "lint": "npm run lint -w service2110",
    "lint:fix": "npm run lint:fix -w service2110",
    "db:generate": "npm run db:generate -w service2110",
    "db:push": "npm run db:push -w service2110",
    "db:seed": "npm run db:seed -w service2110",
    "db:studio": "npm run db:studio -w service2110",
    "docker:db:up": "cd service2110 && docker-compose up postgres -d",
    "docker:db:down": "cd service2110 && docker-compose down",
    "taskViewer:dev": "concurrently -n \"TaskViewer-BE,TaskViewer-FE\" -c \"magenta,green\" \"npm run start:dev -w taskViewerBe\" \"npm run dev -w taskViewerFe\"",
    "kill:port": "node scripts/kill-port.js",
    "kill:3000": "node scripts/kill-port.js 3000",
    "kill:3001": "node scripts/kill-port.js 3001",
    "kill:5173": "node scripts/kill-port.js 5173",
    "kill:all": "npm run kill:3000 && npm run kill:3001 && npm run kill:5173",
    "kill:node": "taskkill /F /IM node.exe 2>nul || echo \"âœ… No node processes running\"",
    "predev": "npm run kill:3000",
    "pretaskViewer:dev": "npm run kill:3001 && npm run kill:5173",
    "docs:generate": "node scripts/generate-swagger-html.js",
    "docs:preview": "npm run docs:generate && echo \"ðŸ“„ Open docs/public/index.html in your browser\"",
    "ci:lint": "npm run lint -w service2110 && npm run lint -w taskViewerBe && npm run lint -w taskViewerFe",
    "ci:build": "npm run build -w service2110 && npm run build -w taskViewerBe && npm run build -w taskViewerFe",
    "ci:check": "npm run ci:lint && npm run ci:build",
    "ai-tasks:run": "node scripts/ai-tasks-runner/runner.js",
    "ai-tasks:dry-run": "node scripts/ai-tasks-runner/runner.js --dry-run"
  },
  "description": "Monorepo with Backend project",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.5",
    "@types/node": "^22.10.5",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.0",
    "drizzle-orm": "^0.45.1",
    "eslint": "^9.18.0",
    "fastify": "^5.7.2",
    "fastify-type-provider-zod": "^6.1.0",
    "globals": "^15.14.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.20.0",
    "zod": "^4.3.6"
  }
}
