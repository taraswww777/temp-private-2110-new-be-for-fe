{
  "name": "be",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "docker-compose up --build",
    "start:dev": "tsx watch src/server.ts",
    "checks": "npm run typecheck && npm run lint && npm run test:run",
    "typecheck": "tsc --noEmit",
    "build:prod": "tsc",
    "build": "npm run checks && npm run build:prod",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Backend application on Fastify + TypeScript + PostgreSQL + Drizzle ORM",
  "devDependencies": {
    "@anatine/zod-openapi": "2.2.8",
    "@eslint/js": "9.39.2",
    "@fastify/cors": "11.2.0",
    "@fastify/swagger": "9.6.1",
    "@fastify/swagger-ui": "5.2.5",
    "@types/node": "25.0.10",
    "@vitest/coverage-v8": "4.0.18",
    "dotenv": "17.2.3",
    "drizzle-kit": "1.0.0-beta.15-859cf75",
    "drizzle-orm": "0.45.1",
    "eslint": "9.39.2",
    "fastify": "5.7.4",
    "fastify-type-provider-zod": "6.1.0",
    "glob": "13.0.3",
    "globals": "17.2.0",
    "pino-pretty": "13.1.3",
    "postgres": "3.4.8",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vitest": "4.0.18",
    "zod": "4.3.6"
  },
  "dependencies": {
    "zod-to-json-schema": "3.25.1"
  }
}
