    "@types/node": "25.0.10",
    "@vitest/coverage-v8": "^2.1.8",
    "dotenv": "17.2.3",
    "drizzle-kit": "0.31.8",
    "drizzle-orm": "0.45.1",
    "eslint": "9.39.2",
    "fastify": "5.7.2",
    "fastify-type-provider-zod": "6.1.0",
    "globals": "17.2.0",
